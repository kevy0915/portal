datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Request {
  id                 String    @id @default(cuid())
  sourceChainId      String
  destChainId        String
  sourceTokenAddress String
  destTokenAddress   String
  amount             Float
  createdAt          DateTime  @default(now())
  updatedAt          DateTime  @updatedAt
  deletedAt          DateTime?
}
